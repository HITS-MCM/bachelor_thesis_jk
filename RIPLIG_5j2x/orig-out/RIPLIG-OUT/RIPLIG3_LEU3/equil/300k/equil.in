# initial structure and coordinates
structure /scratch/mcm/trapp/jobs/pocket_1f2cd40d-8da8-4e03-b579-e495771b7375/RIPLIG-OUT/RIPLIG3_LEU3/equil/300k/heating.psf
coordinates /scratch/mcm/trapp/jobs/pocket_1f2cd40d-8da8-4e03-b579-e495771b7375/RIPLIG-OUT/RIPLIG3_LEU3/equil/300k/heating.coor


# output
outputName equil
binaryOutput no


# forcefield parameters
paraTypeCharmm on
parameters /share/apps/mcm/trapp4/trapptool/auxi/rip-script-NAMD/pdbtool/parms/charmm27.parameter
exclude	scaled1-4
1-4scaling 1.0

# cutoffs
switching on
switchDist 15.0
cutoff 17.0
pairListDist 18

# initial temperature/velocities
velocities /scratch/mcm/trapp/jobs/pocket_1f2cd40d-8da8-4e03-b579-e495771b7375/RIPLIG-OUT/RIPLIG3_LEU3/equil/300k/heating.vel
